<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="login">
	
	<select id="loginCheck" resultType="int" parameterType="logindto">
		select count(*) from member1 where m_id=#{m_id} and m_pass=#{m_pass} and m_level=#{m_level}	
	</select>
	
	<select id="searchNick" resultType="String" parameterType="String">
		select m_nick from member1 where m_id=#{m_id}
	</select>
	
</mapper>
















